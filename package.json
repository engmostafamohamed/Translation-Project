{
  "name": "Translation Project",
  "version": "1.210.0",
  "description": "A template for a nodejs application, using typescript",
  "main": "dist/src/index.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev2": "nodemon --watch src/**/*.ts --exec ts-node ./src/app.ts",
    "dev": "npm run build && nodemon --watch src/**/*.ts --exec ts-node ./src/app.ts",
    "build": "tsc",
    "start": "node ./dist/app.js",
    "lint": "eslint --fix --quiet src/**/*.ts",
    "format": "prettier --loglevel silent --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.2.0",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@babel/generator": "^7.23.0",
    "@types/babel__generator": "^7.6.6",
    "@types/bcrypt": "^5.0.1",
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.9",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@typescript-eslint/typescript-estree": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
